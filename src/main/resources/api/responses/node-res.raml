#%RAML 1.0 DataType
type: object
properties:
  type:
    type: string
    example: Feature
    required: false
  geometry:
    type: object
    properties:
      type:
        type: string
        example: Point
        required: false
      coordinates:
        type: array
        items:
          type: number
  properties:
    type: object
    properties:
      designacao:
        type: string
        example: A10/A13
        required: false
      data_da_ultima_actualizacao:
        type: datetime-only
        example: '2023-08-01T11:32:47'
        required: false
      id:
        type: string
        example: b48a8e39-185f-4c3f-9dc5-b7475b045e87
        required: false
  id:
    type: string
    example: b48a8e39-185f-4c3f-9dc5-b7475b045e87
    required: false
  prev:
    type: string
    example: b3180359-42b9-4a40-b1fe-f8c55d3a7dd2
    required: false
  next:
    type: string
    example: b70b7273-20c0-455e-8b46-52fb7cff6899
    required: false
  links:
    type: array
    items:
      type: object
      properties:
        type:
          type: string
          example: application/json
          required: false
        rel:
          type: string
          example: root
          required: false
        title:
          type: string
          example: The landing page of this server as JSON
          required: false
        href:
          type: string
          example: https://apibrisa.niugis.com?f=json
          required: false
example: |
  {
