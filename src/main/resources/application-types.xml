<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="xml" format="xml">
      <types:example format="xml" element="dataset" location="examples/2-dataset.xml"/>
    </types:type>
    <types:type name="csv" format="csv">
      <types:shape format="weave"><![CDATA[%dw 2.0

type csv = Array<{|  id: Number,   first_name: String,   last_name: String,   email: String,   gender: String,   ip_address: String|}> {"isQuotedString": "false",
"header": "true",
"headerLineNumber": "0"}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
      </types:properties>
    </types:type>
    <types:type name="xmlTocsv" format="csv">
      <types:shape format="weave"><![CDATA[%dw 2.0

type xmlTocsv = Array<{|  id: Number,   name: String,   description: String,   direccion: String,   sexo: String,   CP: String|}> {"isQuotedString": "false",
"header": "true",
"headerLineNumber": "0"}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
      </types:properties>
    </types:type>
    <types:type name="name_file" format="java">
      <types:shape format="raml"><![CDATA[#%RAML 1.0 DataType
type: string]]></types:shape>
    </types:type>
  </types:catalog>
  <types:enrichment select="#090d5f15-8fb7-4485-812a-51cd1d5253d7">
