        </types:message>
      </types:output-event>
    </types:processor-declaration>
  </types:enrichment>
  <types:enrichment select="#c7428499-c864-4986-a9e5-8081ecf3de50"/>
  <types:enrichment select="#1af3de73-4234-4827-bcf6-13c1c90b34b3">
